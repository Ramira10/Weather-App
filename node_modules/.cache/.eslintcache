tainers\\App.js",["80"],"import React, { useState } from 'react';\r\nimport './App.css';\r\nimport Nav from '../components/Nav.jsx';\r\nimport Cards from '../components/Cards.jsx';\r\nimport { Route } from 'react-router-dom';\r\nimport About from '../components/About.jsx';\r\nimport Ciudad from '../components/Ciudad.jsx';\r\nimport Charge from '../components/Charge';\r\n\r\nexport default function App() {\r\n  const [cities, setCities] = useState([]);\r\n\r\n  function onSearch(ciudad) {\r\n    // Lamada a la API externa ---> Datos\r\n    fetch(`http://api.openweathermap.org/data/2.5/weather?q=${ciudad}&appid=3e9e9c2546209a18ca647b0e3c183813&units=metric`)\r\n      .then(r => r.json())\r\n      .then((recurso) => {\r\n        if (recurso.main !== undefined) {\r\n          const ciudad = {\r\n            min: Math.round(recurso.main.temp_min),\r\n            max: Math.round(recurso.main.temp_max),\r\n            img: recurso.weather[0].icon,\r\n            id: recurso.id,\r\n            wind: recurso.wind.speed,\r\n            temp: recurso.main.temp,\r\n            name: recurso.name,\r\n            weather: recurso.weather[0].main,\r\n            clouds: recurso.clouds.all,\r\n            latitud: recurso.coord.lat,\r\n            longitud: recurso.coord.lon\r\n          };\r\n          setCities(oldCities => [...oldCities, ciudad]);\r\n        } else {\r\n          alert(\"Ciudad no encontrada\");\r\n        }\r\n      });\r\n\r\n  }\r\n\r\n  function onClose(id) {\r\n    setCities(oldCities => oldCities.filter(c => c.id !== id));\r\n  }\r\n\r\n  function onFilter(ciudadId) {\r\n    let ciudad = cities.filter(c => c.id === parseInt(ciudadId));\r\n    if(ciudad.length > 0) {\r\n        return ciudad[0];\r\n    } else {\r\n        return null;\r\n    }\r\n  }\r\n\r\n  return (\r\n    <div className=\"App\">\r\n      <Route\r\n        path='/'\r\n        render={() => <Nav onSearch={onSearch} />}\r\n      />\r\n      <Route\r\n        path='/about'\r\n        component={About}\r\n      />\r\n      \r\n      <Route\r\n        exact path='/'\r\n        render={() => <Cards cities={cities} onClose={onClose} />}\r\n      />\r\n\r\n      <Route\r\n      path='/ciudad/:ciudadId'\r\n      render={({match}) => <Ciudad city={onFilter(match.params.ciudadId)}/>}\r\n\r\n      />\r\n       \r\n    </div>\r\n  )\r\n}\r\n\r\n\r\n/* export default App; */\r\n","C:\\Users\\Matías Ramira\\Desktop\\SoyHenry\\M2\\FT-M2\\09-React-Routing\\homework\\src\\components\\Nav.jsx",[],"C:\\Users\\Matías Ramira\\Desktop\\SoyHenry\\M2\\FT-M2\\09-React-Routing\\homework\\src\\components\\Ciudad.jsx",[],"C:\\Users\\Matías Ramira\\Desktop\\SoyHenry\\M2\\FT-M2\\09-React-Routing\\homework\\src\\components\\About.jsx",[],"C:\\Users\\Matías Ramira\\Desktop\\SoyHenry\\M2\\FT-M2\\09-React-Routing\\homework\\src\\components\\Cards.jsx",[],"C:\\Users\\Matías Ramira\\Desktop\\SoyHenry\\M2\\FT-M2\\09-React-Routing\\homework\\src\\components\\SearchBar.jsx",[],"C:\\Users\\Matías Ramira\\Desktop\\SoyHenry\\M2\\FT-M2\\09-React-Routing\\homework\\src\\components\\Card.jsx",[],"C:\\Users\\Matías Ramira\\Desktop\\SoyHenry\\M2\\FT-M2\\09-React-Routing\\homework\\src\\components\\Charge.jsx",[],"C:\\Users\\Matías Ramira\\Desktop\\Weather App\\src\\index.js",[],["81","82"],"C:\\Users\\Matías Ramira\\Desktop\\Weather App\\src\\containers\\App.js",[],"C:\\Users\\Matías Ramira\\Desktop\\Weather App\\src\\components\\Cards.jsx",["83"],"C:\\Users\\Matías Ramira\\Desktop\\Weather App\\src\\components\\About.jsx",["84","85"],"C:\\Users\\Matías Ramira\\Desktop\\Weather App\\src\\components\\Nav.jsx",[],"C:\\Users\\Matías Ramira\\Desktop\\Weather App\\src\\components\\Ciudad.jsx",[],"C:\\Users\\Matías Ramira\\Desktop\\Weather App\\src\\components\\SearchBar.jsx",[],"C:\\Users\\Matías Ramira\\Desktop\\Weather App\\src\\components\\Card.jsx",[],"C:\\Users\\Matías Ramira\\Desktop\\Weather App\\src\\components\\Home.jsx",[],{"ruleId":"86","replacedBy":"87"},{"ruleId":"88","replacedBy":"89"},{